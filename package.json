    "postcss-object-fit-images": "^1.1.2",
    "rimraf": "~2.6.1",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.9.0 "
  },
  "browserify": {
    "transform": [
      "browserify-global-shim"
    ]
  },
  "browser": {
    "jquery-asPieProgress": "./node_modules/jquery-asPieProgress/dist/jquery-asPieProgress.js",
    "slick-carousel": "./node_modules/slick-carousel/slick/slick.js",
    "waypoints": "./node_modules/waypoints/lib/jquery.waypoints.js",
    "sticky-kit": "./node_modules/sticky-kit/dist/sticky-kit.js",
    "hover-intent": "./node_modules/jquery-hoverIntent/jquery.hoverIntent.js",
    "jquery-match-height": "./node_modules/jquery-match-height/dist/jquery.matchHeight.js",
    "jquery-nice-select": "./node_modules/jquery-nice-select/js/jquery.nice-select.js",
    "malihu-custom-scrollbar-plugin": "./node_modules/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.js",
    "masonry-layout": "./node_modules/masonry-layout/masonry.js"
  },
  "browserify-shim": {

  },
  "browserify-global-shim": {
    "jquery": "$",
    "jQuery": "$"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Local Server Settings",
    "host": "127.0.0.1",
    "port": "3008",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_assets/ntu/images",
      "styles": "_assets/ntu/styles",
      "scripts": "_assets/ntu/scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
