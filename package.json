    "jquery-hoverintent": "^1.10.0",
    "jquery-match-height": "^0.7.2",
    "jquery-nice-select": "^1.1.0",
    "jquery-slimscroll": "^1.3.8",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "masonry-layout": "^4.2.2",
    "moment": "^2.21.0",
    "normalize.css": "~5.0.0",
    "object-fit-images": "^3.2.3",
    "pikaday": "^1.7.0",
